{
    "name": "everyworkflow/crm-bundle",
    "description": "Crm bundle for every workflow project.",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "EveryWorkflow",
            "email": "everyworkflow@gmail.com"
        }
    ],
    "require": {
        "everyworkflow/base-bundle": "dev-main",
        "everyworkflow/dam-bundle": "dev-main",
        "everyworkflow/cms-bundle": "dev-main",
        "everyworkflow/customer-bundle": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "EveryWorkflow\\CrmBundle\\": "./"
        },
        "exclude-from-classmap": [
            "Resources",
            "Tests"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "EveryWorkflow\\CrmBundle\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev"
}
